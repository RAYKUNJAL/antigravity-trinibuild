# Database URL
DATABASE_URL="postgresql://user:password@localhost:5432/trinibuild"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_REFRESH_SECRET="your-refresh-token-secret"
JWT_EXPIRY="7d"

# Environment
NODE_ENV="development"
PORT=5000
API_URL="http://localhost:5000"
FRONTEND_URL="http://localhost:3000"

# Stripe
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# TTD Payment Gateways
FCIB_API_KEY="your_fcib_key"
FCIB_MERCHANT_ID="your_fcib_merchant"

REPUBLIC_BANK_API_KEY="your_rep_key"
REPUBLIC_BANK_MERCHANT_ID="your_rep_merchant"

FLOW_MOBILE_API_KEY="your_flow_key"
FLOW_MOBILE_MERCHANT_ID="your_flow_merchant"

NATIONAL_SECURE_API_KEY="your_ns_key"
NATIONAL_SECURE_MERCHANT_ID="your_ns_merchant"

# AWS S3
AWS_ACCESS_KEY_ID="your_key"
AWS_SECRET_ACCESS_KEY="your_secret"
AWS_S3_BUCKET="trinibuild-uploads"
AWS_S3_REGION="us-east-1"

# Email Service (SendGrid)
SENDGRID_API_KEY="your_sendgrid_key"
SENDER_EMAIL="noreply@trinibuild.com"

# SMS Service (Twilio)
TWILIO_ACCOUNT_SID="your_account_sid"
TWILIO_AUTH_TOKEN="your_auth_token"
TWILIO_PHONE_NUMBER="+1868..."

# Redis
REDIS_URL="redis://localhost:6379"

# Google AI
GEMINI_API_KEY="your_gemini_key"

# Logging
LOG_LEVEL="debug"

# Google OAuth (optional)
GOOGLE_CLIENT_ID="your_google_id"
GOOGLE_CLIENT_SECRET="your_google_secret"

# Facebook OAuth (optional)
FACEBOOK_APP_ID="your_fb_id"
FACEBOOK_APP_SECRET="your_fb_secret"
